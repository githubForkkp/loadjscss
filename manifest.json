{
	"manifest_version": 2,
	"name": "GetUnit",
	"version": "1.0",
	"description": "检查用户是否走单元；detail的缓存来源",
	"icons": {
		"32": "image/images256.png",
		"48": "image/images256.png",
		"64": "image/images64.png",
		"128": "image/images128.png"
	},
	"browser_action": {
		"default_icon": "image/images256.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
			"http://detail.tmall.com/*",
			"http://detail.daily.tmall.net/*",
			"http://detail1.pre.tmall.com/*",
			"http://detail2.pre.tmall.com/*"
			],
			"js": ["jquery-1.3.2.min.js","CheckUnit.js"]

		}
	],
	"permissions": [
		"tabs",
		"http://*/*",
		"webRequest"
	],
	"background": {
		"scripts": ["background.js"]
	}
}